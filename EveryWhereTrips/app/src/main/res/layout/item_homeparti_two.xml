<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/dp_20">
    
    <ImageView
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:id="@+id/item_homeparti_two_img"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_15"
        android:src="@mipmap/ic_launcher"/>

    <TextView
        android:id="@+id/item_homeparti_two_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/dp_14"
        android:textColor="@color/c_373737"
        android:layout_marginLeft="@dimen/dp_68"
        android:layout_marginTop="@dimen/dp_10"
        android:text="111"/>
    <TextView
        android:id="@+id/item_homeparti_two_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/dp_10"
        android:textColor="@color/c_999"
        android:text="111"
        android:layout_marginLeft="@dimen/dp_68"
        android:layout_marginTop="@dimen/dp_30"/>

    <TextView
        android:id="@+id/item_homeparti_two_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="111"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/c_999"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_20"/>

    <TextView
        android:id="@+id/item_homeparti_two_countt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/item_homeparti_two_img"
        android:text="111"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/c_636363"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"/>

</RelativeLayout>